<h1>Bienvenido responsable departamento</h1>

    <% asignaciones.forEach(function(asignacion) { %>
        <h1><%= asignacion.asignatura.identificador %></h1>
            <ul>
                <% asignacion.profesores.forEach(function(profesor){%>
                    <li><%= profesor %></li>
                <% }); %>
            </ul>
    <% }); %>

